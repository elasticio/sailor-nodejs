{
  "name": "elasticio-sailor-nodejs",
  "description": "The official elastic.io library for bootstrapping and executing for Node.js connectors",
  "version": "3.0.0-dev2",
  "main": "run.js",
  "scripts": {
    "build": "tsc",
    "audit": "better-npm-audit audit --level high --production",
    "pretest": "npm run lint",
    "lint": "eslint --ext .ts .",
    "test": "npm run test:jasmine && npm run test:mocha",
    "test:jasmine": "NODE_ENV=test jasmine-node spec",
    "test:mocha": "NODE_ENV=test node_modules/.bin/mocha --recursive mocha_spec",
    "postpublish": "./postpublish.js",
    "dev:local": "node run.local.js | bunyan"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "dependencies": {
    "bunyan": "1.8.10",
    "co": "4.6.0",
    "debug": "3.1.0",
    "elasticio-rest-node": "2.0.0",
    "event-to-promise": "0.8.0",
    "lodash": "4.17.21",
    "p-throttle": "2.1.0",
    "q": "1.5.1",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "better-npm-audit": "3.7.3",
    "chai": "4.2.0",
    "del": "2.2.2",
    "eslint": "4.19.1",
    "eslint-plugin-mocha": "4.12.1",
    "express": "4.16.4",
    "gulp": "3.9.1",
    "gulp-istanbul": "1.1.3",
    "gulp-jasmine": "0.2.0",
    "jasmine-node": "3.0.0",
    "mocha": "7.1.2",
    "nock": "12.0.3",
    "rabbitmq-stats": "1.2.4",
    "request": "2.88.0",
    "request-promise-native": "1.0.5",
    "sinon": "9.0.2",
    "sinon-chai": "3.5.0",
    "ts-node": "10.4.0",
    "typescript": "4.4.4"
  },
  "repository": "elasticio/sailor-nodejs",
  "license": "Apache-2.0"
}
