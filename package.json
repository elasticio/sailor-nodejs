{
  "name": "elasticio-sailor-nodejs",
  "description": "The official elastic.io library for bootstrapping and executing for Node.js connectors",
  "version": "2.5.1",
  "main": "run.js",
  "scripts": {
    "test": "mocha spec/*.js",
    "integration-test": "mocha spec-integration/**/*.js",
    "coverage": "nyc mocha spec/*.js --timeout=3000 && nyc report --reporter=text-lcov | coveralls",
    "postpublish": "./postpublish.js"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "dependencies": {
    "amqplib": "0.5.1",
    "bunyan": "1.8.10",
    "co": "4.6.0",
    "debug": "3.1.0",
    "elasticio-rest-node": "1.2.3",
    "event-to-promise": "0.8.0",
    "lodash": "4.17.15",
    "p-throttle": "2.1.0",
    "q": "1.4.1",
    "request-promise-native": "1.0.5",
    "requestretry": "3.1.0",
    "self-addressed": "0.3.0",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "coveralls": "3.0.7",
    "dotenv": "8.2.0",
    "eslint": "6.6.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-mocha": "6.2.1",
    "mocha": "6.2.0",
    "mocha-lcov-reporter": "1.3.0",
    "nock": "10.0.6",
    "nyc": "14.1.1",
    "request": "2.88.0",
    "sinon": "7.5.0",
    "sinon-chai": "3.3.0"
  },
