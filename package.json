    "eslint": "^4.18.2",
    "eslint-plugin-mocha": "^4.12.1",
    "express": "4.14.1",
    "gulp": "3.9.1",
    "gulp-istanbul": "1.1.1",
    "gulp-jasmine": "0.2.0",
