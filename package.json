{
  "name": "elasticio-sailor-nodejs",
  "description": "The official elastic.io library for bootstrapping and executing for Node.js connectors",
  "version": "3.0.0",
  "main": "src/run.js",
  "repository": "elasticio/sailor-nodejs",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "npm run test:jasmine && npm run test:mocha",
    "test:jasmine": "NODE_ENV=test jasmine --config=spec-jasmine/support/jasmine.json",
    "test:mocha": "NODE_ENV=test mocha --opts spec-mocha/mocha.opts",
    "postpublish": ".circleci/postpublish.js"
  },
  "dependencies": {
    "amqplib-as-promised": "3.15.1",
    "bunyan": "1.8.12",
    "debug": "4.1.1",
    "elasticio-rest-node": "1.2.2",
    "lodash": "4.17.11",
    "request": "2.88.0",
    "request-promise-native": "1.0.7",
    "requestretry": "3.1.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "eslint": "5.15.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-mocha": "5.3.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "heapdump": "0.3.12",
    "jasmine": "3.3.1",
    "mocha": "6.0.2",
    "nock": "10.0.6",
    "sinon": "7.2.6"
  }
}
